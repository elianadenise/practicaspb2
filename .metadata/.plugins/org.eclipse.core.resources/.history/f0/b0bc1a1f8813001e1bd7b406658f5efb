		this.tipo = tipo;
		this.numeroDeInscripcion = numeroDeInscripcion;
		this.participantes = new TreeMap<Integer, Deportista>();
	}
	
	public inscribirDeportista(Deportista deportista) {
		
	}
	
	public Integer cantidadDeParticipantes() {
		this.participantes.size();
	}
}